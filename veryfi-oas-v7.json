  "paths": {
    "/documents": {
      "get": {
        "tags": [
          "documents"
        ],
        "summary": "Search Documents",
        "description": "Retrieve previously processed documents.\n",
        "operationId": "getDocuments",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Search term to search for a specific document by its content.\nThese fields will be searched:\nexternal_id, category, vendor.name, notes, invoice_number, total and ocr_text.\n",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "external_id",
            "in": "query",
            "description": "Search for documents that match your custom identifier.",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "created__gt",
            "in": "query",
            "description": "Search for documents with a created date greater than the one specified here.\nThe date format for this parameter is: YYYY-MM-DD+HH:MM:SS\nDon't send both `created__gt` and `created__gte` in a single request.\n",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "created__gte",
            "in": "query",
            "description": "Search for documents with a created date greater than or equal to the one specified here.\nThe date format for this parameter is: YYYY-MM-DD+HH:MM:SS\nDon't send both `created__gt` and `created__gte` in a single request.\n",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "created__lt",
            "in": "query",
            "description": "Search for documents with a created date less than the one specified here.\nThe date format for this parameter is: YYYY-MM-DD+HH:MM:SS\nDon't send both `created__lt` and `created__lte` in a single request.\n",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "created__lte",
            "in": "query",
            "description": "Search for documents with a created date less than or equal to the one specified here.\nThe date format for this parameter is: YYYY-MM-DD+HH:MM:SS\nDon't send both `created__lt` and `created__lte` in a single request.\n",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of previously processed documents.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Document"
                  }
                }
              }
            }
          },
          "default": {
            "description": "OperationStatus.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OperationStatus"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "documents"
        ],
        "summary": "Process a Document.",
        "description": "Process a document and extract all the fields from it.\n",
        "operationId": "processDocument",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            },
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DocumentUploadOptions"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Processed document response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "default": {
            "description": "OperationStatus.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OperationStatus"
                }
              }
            }
          }
        }
      }
    },
    "/documents/{document_id}": {
      "get": {
        "tags": [
          "documents"
        ],
        "summary": "Get a Document.",
        "description": "Retrieve a previously processed document.",
        "operationId": "getDocument",
        "parameters": [
          {
            "name": "document_id",
            "in": "path",
            "description": "ID of the document.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "return_audit_trail",
            "in": "query",
            "description": "Set to `1` to return the Audit Trail on this document. An audit trail includes information about changes made to this document and by who by date.",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Processed document response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "default": {
            "description": "OperationStatus.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OperationStatus"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "documents"
        ],
        "summary": "Update a Document.",
        "description": "Update a previously processed document. Since Veryfi's Machine Learning models undergo frequent training, changes that are made to processed documents are used during these training sessions and eventually help improve the accuracy of data extraction.",
        "operationId": "updateDocument",
        "parameters": [
          {
            "name": "document_id",
            "in": "path",
            "description": "ID of the document.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "$ref": "#/components/requestBodies/DocumentUpdateOptions"
        },
        "responses": {
          "200": {
            "description": "Document.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "default": {
            "description": "OperationStatus.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OperationStatus"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "documents"
        ],
        "summary": "Delete a Document.",
        "description": "Delete a processed document from Veryfi.",
        "operationId": "deleteDocument",
        "parameters": [
          {
            "name": "document_id",
            "in": "path",
            "description": "ID of the document.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "OperationStatus.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OperationStatus"
                }
              }
            }
          }
        }
      }
    },
